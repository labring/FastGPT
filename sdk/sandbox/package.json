    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "sandbox",
    "cloud",
    "adapter",
    "abstraction"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@alibaba-group/opensandbox": "^0.1.3"
  },
  "devDependencies": {
    "vitest": "^3.0.9",
    "@vitest/coverage-v8": "^3.0.9",
    "typescript": "^5.1.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
